{
	"name": "bybil",
	"type": "module",
	"version": "0.0.11",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/rujorgensen/bybil.git"
	},
	"bugs": "https://github.com/rujorgensen/bybil/issues",
	"homepage": "https://bybil.nu",
	"scripts": {
		"dev": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro",
		"wrangler:dev": "wrangler pages dev",
		"pwa:generate-assets": "pwa-assets-generator --preset minimal public/logo.svg",
		"docker:build": "docker build -t bybil:latest -f .deploy/Dockerfile .",
		"docker:run": "docker run --rm -p 6122:6122 bybil:latest",
		"dock": "bun docker:build && bun docker:run",
		"upd": "bun upgrade && ncu -i --format group",
		"format": "biome format --write",
		"qr:generate": "qrcode -d 09366D -o ./data/logo-and-qr/qr-code.svg --type=svg \"https://bybil.nu/r/ek42435\""
	},
	"dependencies": {
		"@astrojs/markdoc": "^0.15.1",
		"@astrojs/node": "9.3.0",
		"@astrojs/react": "4.3.0",
		"@astrojs/rss": "^4.0.12",
		"@astrojs/sitemap": "^3.4.1",
		"@astrojs/tailwind": "6.0.2",
		"@keystatic/astro": "^5.0.6",
		"@keystatic/core": "^0.5.47",
		"astro": "5.12.0",
		"discord.js": "^14.21.0",
		"keystatic-components": "^0.0.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@bobthered/tailwindcss-palette-generator": "^4.0.0",
		"@iconify-json/ic": "^1.2.2",
		"@iconify-json/iconamoon": "^1.2.2",
		"@iconify-json/mdi": "^1.2.3",
		"@iconify-json/ph": "^1.2.2",
		"@iconify-json/solar": "^1.2.2",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@types/bun": "^1.2.18",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vite-pwa/assets-generator": "^1.0.0",
		"@vite-pwa/astro": "^1.1.0",
		"astro-icon": "^1.1.5",
		"astro-robots-txt": "^1.0.0",
		"astro-seo": "^0.8.4",
		"qrcode": "^1.5.4",
		"satori": "^0.15.2",
		"sharp": "0.34.3",
		"tailwindcss": "^3.4.3",
		"workbox-window": "^7.3.0"
	},
	"packageManager": "bun@1.2.19",
	"overrides": {
		"sharp": "0.34.3"
	}
